<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<g:DialogBox modal="true" width="500px" height="245px">
		<g:caption>
			<b>Popup de recherche</b>
		</g:caption>
		<g:HTMLPanel width="460px" height="199px">
			<g:LayoutPanel width="460px" height="180px">
				<g:layer left="0px" width="500px" top="15px" height="76px">
					<g:VerticalPanel width="460px" height="40px">
						<g:HorizontalPanel width="323px"
							verticalAlignment="ALIGN_MIDDLE">
							<g:Label text="Rechercher:" horizontalAlignment="ALIGN_RIGHT"
								styleName="gwt-RichTextToolbar" />
							<g:SuggestBox ui:field="autoCompleteBox" focus="true" />
						</g:HorizontalPanel>
					</g:VerticalPanel>
				</g:layer>
				<g:layer left="0px" width="460px" top="136px" height="44px">
					<g:HorizontalPanel spacing="10" width="460px"
						horizontalAlignment="ALIGN_CENTER">
						<g:Button ui:field="annulerButton" text="Annuler" width="102" />
					</g:HorizontalPanel>
				</g:layer>
			</g:LayoutPanel>
		</g:HTMLPanel>

	</g:DialogBox>
</ui:UiBinder>
